package com.krakedev;

public class Calculadora {
	
	public int sumar(int valor1, int valor2) {
		int resultado; // declaraci√≥n de una variable
		resultado = valor1+valor2;
		return resultado;
	}
	
	public double restar(int valor1, int valor2) {
		int resultado;
		resultado = valor1-valor2;
		return resultado;
	}
}
